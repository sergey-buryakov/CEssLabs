#include <iostream>

using namespace std;

int main()
{
    int x1, x2;
    cout << "Enter first: ";
    cin >> x1;
    cout << "Enter second: ";
    cin >> x2;
    if (1/(float)x1 == 1/(float)x2)
        cout << "Results are equal (by 0.000001 epsilon)";
    else
        cout << "Results are not equal (by 0.000001 epsilon)" << endl;
    cout << 1/(float)x1 << "\t" << 1/(float)x2;
    return 0;
}
